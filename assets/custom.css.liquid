.input-holder {
  position: relative;
  margin-bottom: 15px;
}

.input-holder span.error {
  position: absolute;
  left: 0;
  background: #f00;
  padding: 3px 5px;
  font-size: 13px;
  border-radius: 2px;
  color: #fff;
  font-weight: 400;
  bottom: 50px;
}

.input-holder span.error:after {
  position: absolute;
  border-top: 6px solid #f00;
  border-left: 6px solid transparent;
  border-right: 6px solid transparent;
  bottom: -4px;
  left: 3px;
  content: '';
}

.textarea_holder span.error {
  bottom: auto;
  top: -31px;
}

